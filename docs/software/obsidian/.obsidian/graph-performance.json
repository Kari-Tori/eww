{
  "collapse-filter": true,
  "search": "-path:archive -path:Tags -path:.obsidian",
  "showTags": false,
  "showAttachments": false,
  "hideUnresolved": true,
  "showOrphans": false,
  "collapse-color-groups": false,
  "colorGroups": [
    {
      "query": "path:karinam OR tag:#candy OR tag:#pink",
      "color": {"a": 1, "rgb": 16711935}
    },
    {
      "query": "path:jakubc OR tag:#owner OR tag:#eww",
      "color": {"a": 1, "rgb": 65535}
    },
    {
      "query": "tag:#core OR path:core",
      "color": {"a": 1, "rgb": 16711680}
    },
    {
      "query": "path:business OR tag:#business",
      "color": {"a": 1, "rgb": 16776960}
    },
    {
      "query": "path:docs OR tag:#docs",
      "color": {"a": 1, "rgb": 65280}
    }
  ],
  "collapse-display": false,
  "showArrow": false,
  "textFadeMultiplier": -1,
  "nodeSizeMultiplier": 0.8,
  "lineSizeMultiplier": 1,
  "collapse-forces": false,
  "centerStrength": 0.5,
  "repelStrength": 15,
  "linkStrength": 0.5,
  "linkDistance": 200,
  "scale": 1,
  "close": false
}
